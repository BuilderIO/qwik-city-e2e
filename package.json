{
  "name": "qwik-city-e2e",
  "private": true,
  "scripts": {
    "build.client": "vite build",
    "build.cloudflare": "npm run build.client && vite build -c adaptors/cloudflare-pages/vite.config.ts",
    "build.express": "vite build -c adaptors/express/vite.config.ts",
    "build.netlify": "npm run build.client && vite build -c adaptors/netlify-edge/vite.config.ts",
    "build.vercel": "npm run build.client && vite build -c adaptors/vercel-edge/vite.config.ts",
    "build.preview": "vite build --minify=false --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "PLATFORM_NAME=dev vite --mode ssr --port 3000",
    "dev.debug": "PLATFORM_NAME=dev node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force --port 3000",
    "dev.netlify": "netlify dev",
    "deploy.netlify": "netlify deploy --prod --build",
    "deploy.vercel": "vercel deploy --prod",
    "serve.cloudflare": "wrangler pages dev ./dist",
    "serve.cloudflare.debug": "node --inspect-brk ./node_modules/.bin/wrangler pages dev ./dist",
    "serve.express": "node .express/entry.express.js",
    "serve.express.debug": "node --inspect-brk .express/entry.express.js",
    "serve.preview": "vite preview --port 3001",
    "serve.preview.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js preview --port 3001",
    "test.cloudflare.mpa": "APP=mpa playwright test tests --config playwright.cloudflare.config.ts",
    "test.cloudflare.spa": "APP=spa playwright test tests --config playwright.cloudflare.config.ts",
    "test.dev.mpa": "APP=mpa playwright test tests --config playwright.config.ts",
    "test.dev.spa": "APP=spa playwright test tests --config playwright.config.ts",
    "test.express.mpa": "APP=mpa playwright test tests --config playwright.express.config.ts",
    "test.express.spa": "APP=spa playwright test tests --config playwright.express.config.ts",
    "test.netlify.mpa": "APP=mpa playwright test tests --config playwright.netlify.config.ts",
    "test.netlify.spa": "APP=spa playwright test tests --config playwright.netlify.config.ts",
    "test.preview.mpa": "APP=mpa playwright test tests --config playwright.preview.config.ts",
    "test.preview.spa": "APP=spa playwright test tests --config playwright.preview.config.ts",
    "test.vercel.mpa": "APP=mpa playwright test tests --config playwright.vercel.config.ts",
    "test.vercel.spa": "APP=spa playwright test tests --config playwright.vercel.config.ts",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.17.0",
    "@builder.io/qwik-city": "0.1.0-beta10",
    "@builder.io/qwik-react": "0.2.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/x-data-grid": "5.17.8",
    "@playwright/test": "^1.29.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "latest",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "netlify-cli": "^12.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4",
    "undici": "^5.16.0",
    "vercel": "^28.12.5",
    "vite": "4.0.4",
    "wrangler": "^2.8.0"
  }
}
