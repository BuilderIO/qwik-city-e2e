{
  "name": "qwik-city-e2e",
  "private": true,
  "scripts": {
    "build.client": "vite build",
    "build.cloudflare": "npm run build.client && vite build -c adaptors/cloudflare-pages/vite.config.ts",
    "build.express": "vite build -c adaptors/express/vite.config.ts",
    "build.netlify": "npm run build.client && vite build -c adaptors/netlify-edge/vite.config.ts",
    "build.vercel": "npm run build.client && vite build -c adaptors/vercel-edge/vite.config.ts",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr --port 3000",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force --port 3000",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "deploy.netlify": "netlify deploy",
    "deploy.vercel": "vercel deploy --prod",
    "serve.cloudflare": "wrangler pages dev ./dist",
    "serve.cloudflare.debug": "node --inspect-brk ./node_modules/.bin/wrangler pages dev ./dist",
    "serve.express": "node .express/entry.express.js",
    "serve.express.debug": "node --inspect-brk .express/entry.express.js",
    "serve.preview": "vite preview --port 3001",
    "start": "vite --open --mode ssr",
    "test.dev": "playwright test tests --config playwright.config.ts",
    "test.express": "playwright test tests --config playwright.express.config.ts",
    "test.preview": "playwright test tests --config playwright.preview.config.ts",
    "test.vercel": "playwright test tests --config playwright.vercel.config.ts",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "BuilderIO/qwik-build#72145630a8c4a02a779532c5e2e2590fb52946bc",
    "@builder.io/qwik-city": "BuilderIO/qwik-city-build#ae90edba8976b56d65a926c1e257130ee35e3d27",
    "@playwright/test": "^1.29.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.4.10",
    "@types/express": "^4.17.14",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "netlify-cli": "^12.7.2",
    "node-fetch": "3.3.0",
    "prettier": "2.8.2",
    "typescript": "4.9.4",
    "vercel": "^28.11.1",
    "vite": "4.0.4",
    "vite-tsconfig-paths": "4.0.3",
    "wrangler": "^2.7.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
